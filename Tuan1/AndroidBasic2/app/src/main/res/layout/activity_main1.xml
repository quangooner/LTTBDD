<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    >
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        >
        <Button
            android:id="@+id/go_button1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Go to Example1 Activity"
            android:layout_gravity="center_horizontal"
            />
        <Button
            android:id="@+id/go_button2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Go to Example2 Activity"
            android:layout_gravity="center_horizontal"
            />

        <Button
            android:id="@+id/go_button3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Go to Example3 Activity"
            android:layout_gravity="center_horizontal"
            />

        <Button
            android:id="@+id/go_button4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Go to Example4 Activity"
            android:layout_gravity="center_horizontal"
            />
        <Button
            android:id="@+id/go_button5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Go to Example5 Activity"
            android:layout_gravity="center_horizontal"
            />

    </LinearLayout>
</RelativeLayout>